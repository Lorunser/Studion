@model System.String

@{
    //does not work for localhost as localhost is not routable but will work when hosted online
    string url = "https://docs.google.com/gview?url=" + Request.Url.AbsoluteUri.Replace(Request.Url.AbsolutePath, "") + Model +"&embedded=true";
}

<iframe src="@url"
        style="width: 100%; height: 80%">
</iframe>
